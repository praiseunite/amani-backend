[flake8]
max-line-length = 100
extend-ignore = E203, E266, E501, W503, E704
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    .tox,
    alembic/versions
max-complexity = 10
docstring-convention = google
per-file-ignores =
    __init__.py:F401
    tests/*:D100,D101,D102,D103,D104
